<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    <bean id="outputHelper" class="spring.core.OutputHelper">
    	<property name="outputService" ref="jsonOutput"></property>
    </bean>

   <bean id="jsonOutput" class="spring.core.JsonOutput"></bean>
   <bean id="textOutput" class="spring.core.TextOutput"></bean>
   

</beans>